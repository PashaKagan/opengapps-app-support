    <!--______APP INTRO______-->
        <!--TITLES-->
            <string name="appintro_introslide_title">ברוכים הבאים לאפליקציה הרשמית של OpenGApps.org</string>
            <string name="appintro_storageperm_title">הרשאה לשמירת קבצים</string>
            <string name="appintro_root_title">הרשאת רוט</string>
            <string name="appintro_label_architecture">ארכיטקטורת מעבד</string>
            <string name="appintro_label_android">גרסת אנדרואיד</string>
            <string name="appintro_label_variant">גרסת חבילה</string>
            <string name="label_tos">תנאי שימוש</string>
        <!--DESCRIPTIONS-->
            <string name="appintro_storageperm_explain">על מנת להוריד חבילה של Open GApps, עליך לאפשר לאפליקציה גישה לאחסון.</string>
            <string name="slide_arch_description">יש לבחור את ארכיטקטורת המעבד שתהיה בשימוש עבור חבילת ה-Open GApps</string>
            <string name="slide_android_description">יש לבחור את גרסת האנדרואיד שתהיה בשימוש עבור חבילת ה-Open GApps</string>
            <string name="slide_variant_description">יש לבחור את גרסת החבילה של ה-Open GApps שתורד</string>
            <string name="appintro_root_explanation">האפליקציה מאפשרת דרך מובנית להתקין חבילות שהורדו. אפשרות התקנה זו דורשת <i>הרשאת רוט</i>. כדי להשתמש בה, יש לאפשר הרשאת רוט לאפליקציה. \nשלב זה אינו שלב חובה.</string>
            <string name="appintro_terms_explain">החבילות המוכנות מראש מאתר OpenGApps.org מוצעות אך ורק באדיבות OpenGApps.org ללא אחריות משום סוג, תחת התנאים המתירים שימוש חופשי לצורך אישי בלבד, ואינם מאפשרים שיתוף משום אתר שאינו OpenGApps.org. \n \n יש לציין כי Open GApps אינם מספקים כל רישיון לשימוש בקבצי APK של Google הנמצאים בחבילה זו. חבילות ה-Open GApps מציעות דרך נוחה להתקין קבצי APK במכשירך. באחריות המשתמש הבלעדית להשיג הרשאה הרשאת שימוש, לדוגמא: על ידי רכישת מכשיר בעל רישיון ואפליקציות גוגל מותקנות מראש ו/או הורדת האפליקציות מה-Google Play Store.</string>
        <!--HINTS-->
            <string name="slide_arch_hint">האפליקציה מזהה אוטומטית את הארכיטקטורה של מכשירך</string>
            <string name="slide_android_hint">האפליקציה מזהה אוטומטית את גרסת האנדרואיד של מכשירך</string>
            <string name="slide_variant_hint">האפליקציה מזהה אוטומטית את גרסת החבילה המותקנת על מכשירך</string>
